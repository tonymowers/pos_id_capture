			</Fields>
			<Fields id="Sex">
				<Default rule="SexInitials" />
			</Fields>
			<Fields id="Weight">
				<Default rule="Weight-Barcode-LB" />
				<Exception iin="GRP001" rule="Weight-Barcode-KG" />
			</Fields>
			<Fields id ="Height">
				<Default rule="Height-Barcode-Feet-Inches" />
