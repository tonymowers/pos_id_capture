		<!-- MN Magstrip data -->
		<Template version="64">			
			<Fields id="LicenceNumber">
				<Default rule="LicenceNumber" />
			</Fields>
			<Fields id="Address">
				<Default rule="Address-MagStripe" />
				<Exception iin="GRP001" rule="Address-MagStripeCA" desc="Adds spaces to PostalCodes" />
			</Fields>
			<Fields id="Dates">
				<Default rule="DatesYearMonthDayMAG" />
			</Fields>
			<Fields id="NameParts">
